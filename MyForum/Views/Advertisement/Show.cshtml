@model MyForum.ViewModel.AdvertisementShowView

<div style="color:#3F3F46;">
    <p style="color:#3F3F46;">請關閉AdBlock</p>
    @foreach (var item in Model.URLs)
    {
        <a href="@item.URL" style="color:#3F3F46;">
            <img src="@Url.Content("~/Ad-Uploads/" + item.PictPath)" alt="ADID=@item.AD_ID">
        </a>
    }
</div>
